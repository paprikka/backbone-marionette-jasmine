<h2 class="user-edit-title">
  Edit contact
  <a href="#!/" class="cancel-btn"><i class="icon-close"></i> Cancel</a>
  <span class="save-and-close-btn"><i class="icon-pencil"></i> Save</span>
</h2>
<div class="ui-form-group">
  <label class="ui-form-label">First name</label>
  <input type="text" value="{{firstName}}"  class="ui-form-input user-edit-first-name">
</div>
<div class="ui-form-group">
  <label class="ui-form-label">Last Name</label>
  <input type="text" value="{{lastName}}" class="ui-form-input user-edit-last-name">
</div>
<div class="ui-form-group">
  <label class="ui-form-label">Avatar URL</label>
  <input type="text" value="{{avatarUrl}}" class="ui-form-input user-edit-avatar-url">
</div>


<div class="ui-form-group">
  <label class="ui-form-label">Current job position</label>
  <input type="text" value="{{currentPosition}}" class="ui-form-input user-edit-current-position">
</div>

<div class="ui-form-group">
  <label class="ui-form-label">Description</label>
  <textarea cols="30" rows="3" class="ui-form-input user-edit-description">{{description}}</textarea>
</div>

<div class="ui-form-group">
  <label class="ui-form-label">Phone</label>
  <input type="text" value="{{phone}}" class="ui-form-input user-edit-phone">
</div>
<div class="ui-form-group">
  <label class="ui-form-label">Email</label>
  <input type="text" value="{{email}}" class="ui-form-input user-edit-email">
</div>
<div class="ui-form-group">
  <label class="ui-form-label">Twitter</label>
  <input type="text" value="{{twitterHandle}}" class="ui-form-input user-edit-twitter">
</div>